                            }
                        </div>

                        <div class="col-md-3">
                            @Html.LabelFor(model => model.CiudadId, htmlAttributes: new { @class = "control-label col-md-12" })
                            @Html.DropDownListFor(model => model.CiudadId, new SelectList(string.Empty, "Value", "Text"), "Seleccione...", new { @class = "form-control" })
                            @if (ViewBag.IsValid != null && ViewBag.IsValid == "false")
                            {
                                @Html.ValidationMessageFor(model => model.UnidadNegocio, "Seleccione una Ciudad.", new { @class = "text-danger" })

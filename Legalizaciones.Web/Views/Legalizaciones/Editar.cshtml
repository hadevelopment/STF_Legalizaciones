                        {
                            @Html.ValidationMessageFor(model => model.MonedaId, "Seleccione un tipo de Moneda.", new { @class = "text-danger" })
                        }

                    </div>
                </div>

                <div class="row margin-bottom">
                    <div class="box-tools pull-right" style="margin-right: 20px;">
                        <a id="btnA" class="btn btn-primary" onclick="AnadirGastos(this);" style="float:right;">
                            <span class="glyphicon glyphicon-plus"></span> Añadir Gasto
                        </a>
                    </div>
                </div>

                <div class="row display-none" id="mensajeValidacionRegistro">
                    <div class="col-sm-12">
                        <div class="alert alert-warning" role="alert">
                            <span id="mensajeRegistro"></span>
                        </div>
                    </div>
                </div>


            </div>


            @*formulario de legalizacion de gastos*@
            <div class="hide" id="RegistroDatos">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class=""></i><strong> Registro de Datos</strong> </h3>
                </div>

                <div class="box-body">

                    <div class="row margin-bottom">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.GastosId, htmlAttributes: new { @class = "control-label col-md-12" })
                            @Html.EditorFor(model => model.GastosId, new { htmlAttributes = new { @class = "form-control" } })
                            @Html.ValidationMessageFor(model => model.GastosId, "", new { @class = "text-danger" })

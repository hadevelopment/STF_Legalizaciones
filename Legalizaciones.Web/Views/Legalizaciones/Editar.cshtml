                    <div class="row margin-bottom">

                        <div class="col-md-3">
                            @Html.LabelFor(model => model.TiposervicioId, htmlAttributes: new { @class = "control-label col-md-12" })
                            @Html.DropDownListFor(model => model.TiposervicioId, new SelectList(string.Empty, "Value", "Text"), "Seleccione...", new { @class = "form-control" })

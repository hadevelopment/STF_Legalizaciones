@model Legalizaciones.Web.Models.ViewModel.LegalizacionesViewModel;
@{
    ViewData["Title"] = "Editar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Editar</h2>

<div class="row">
    <div class="col-sm-12">
        <div class="box">
            <div class="box-body">
                @if (Model.ConAnticipo == 1)
                {
                    <small>
                        <a href='@Url.Action("Index","Home")'> Inicio </a>
                        >
                        <a href='@Url.Action("Index","Legalizaciones")'> Legalizaciones </a>
                        >
                        <strong>Registro legalización anticipo @Model.AnticipoId</strong>
                    </small>

                }
                else
                {
                    <small>
                        <a href='@Url.Action("Index","Home")'> Inicio </a>
                        >
                        <a href='@Url.Action("Index","Legalizaciones")'> Registro Legalizacion sin anticipo </a>
                        >
                        <strong>Nueva</strong>
                    </small>

                }
            </div>
        </div>
    </div>
</div>

<form asp-controller="Legalizaciones" asp-action="Actualizar" method="post">
    <section class="">
        <div class="hidden">
            @Html.TextBoxFor(model => model.ConAnticipo, new { @class = "form-control" })
        </div>


        @if (Model.ConAnticipo == 1)
        {
            @*informacion de la solicitud anticipo*@
            <div class="box box-default">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="glyphicon glyphicon-credit-card"></i><strong> Información del Anticipo</strong></h3>
                </div>
                @Html.HiddenFor(model => model.AnticipoId, null)
                <div class="box-body">
                    <table class="tableSolicitud">
                        <thead>
                            <tr>
                                <th class="col-md-1">
                                    @Html.DisplayNameFor(modelItem => modelItem.AnticipoId)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.DocumentoERPID)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.FechaRegistro)
                                </th>
                                <th class="col-md-3">
                                    @Html.DisplayNameFor(modelItem => modelItem.Concepto)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.FechaVencimiento)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.Monto)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.AnticipoId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.DocumentoERPID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.FechaRegistro)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Concepto)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.FechaVencimiento)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Monto)
                                </td>

                            </tr>
                        </tbody>
                    </table>


                </div>
            </div>

        }

        @if (Model.ConAnticipo == 0)
        {
            @*informacion del empleado*@
            <div class="box box-default">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-info-circle"></i><strong>Información del Empleado</strong> </h3>
                </div>

                <div class="box-body">
                    @Html.LabelFor(model => model.CedulaId, htmlAttributes: new { @class = "control-label col-md-12" })
                    @Html.DropDownListFor(model => model.CedulaId, Model.ListaEmpleado, new { @class = "form-control" })
                    @if (ViewBag.IsValid != null && ViewBag.IsValid == "false")
                    {
                        @Html.ValidationMessageFor(model => model.CedulaId, "Seleccione un Empleado.", new { @class = "text-danger" })
                    }
                </div>
            </div>
        }
        else
        {
            @*informacion del empleado*@
            <div class="box box-default">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-info-circle"></i><strong>Información del Empleado</strong> </h3>
                </div>

                <div class="box-body">

                    <table class="tableSolicitud" id="TablaEmpleado">
                        <thead>
                            <tr>
                                <th class="col-md-4">
                                    @Html.DisplayNameFor(modelItem => modelItem.Nombre)
                                </th>
                                <th class="col-md-3">
                                    @Html.DisplayNameFor(modelItem => modelItem.Cedula)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.Area)
                                </th>
                                <th class="col-md-2">
                                    @Html.DisplayNameFor(modelItem => modelItem.Cargo)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Nombre)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Cedula)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Area)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => modelItem.Cargo)
                                </td>
                            </tr>
                        </tbody>
                    </table>


                </div>
            </div>
        }



        @*Esta es la parte de legalizaciones*@

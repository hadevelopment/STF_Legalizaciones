        @*seccion de los gastos ya legalizados*@
        <div class="box box-default">
            <div class="box-header with-border">
            </div>

            <div class="box-body">

                <div class="row">
                    <div class="col-md-12">
                        <table class="tableGastos" id="tbGastos">
                            <thead>
                                <tr>
                                    @*primero voy a colocar todos los campos que necesito para deserealizar los gastos*@
                                    <td>Item</td>
                                    <td>Fecha</td>
                                    <td>Centro Oper.</td>
                                    <td>Unidad Neg</td>
                                    <td>Centro Cost</td>
                                    <td>Fecha Gasto</td>
                                    @*<td>MotivoId</td>*@
                                    <td>Pais</td>
                                    <td>Ciudad</td>
                                    <td>Servicio</td>
                                    <td>Proveedor</td>
                                    <td>Concepto</td>
                                    <td>Valor</td>
                                    <td>Acciones</td>
                                </tr>
                            </thead>
                            
                                @if (Model.LegalizacionGastos.Count > 0)
                                {
                                    @foreach (var item in Model.LegalizacionGastos)
                                    {
                                        // string total = (item.Monto + Convert.ToDecimal(item.IVA)).ToString("N2");
                                        <tr>
                                            <td class="col-md-1 ">@item.Id</td>
                                            <td class="col-md-1 ">@String.Format("{0:dd-MM-yyy}", item.FechaCreacion)</td>
                                            <td class="col-md-2 ">@item.CentroOperacion.Nombre</td>
                                            <td class="col-md-2 ">@item.UnidadNegocio.Nombre</td>
                                            <td class="col-md-2 ">@item.CentroCosto.Nombre</td>
                                            <td class="col-md-2 ">@String.Format("{0:dd-MM-yyy}", item.FechaGasto)</td>
                                            @*<td class="col-md-1 ">0</td>*@
                                            <td class="col-md-1 ">@item.Pais.Nombre</td>
                                            <td class="col-md-1 ">@item.Ciudad.Nombre</td>
                                            <td class="col-md-1 ">

                                                @if (@item.TipoServicioId == 1)
                                                {
                                                    <span>Comida</span>
                                                }
                                                else
                                                {
                                                    <span>Transporte</span>
                                                }
                                            </td>
                                            <td class="col-md-1 ">@item.ProveedorId</td>
                                            <td class="col-md-1 ">@item.Concepto</td>
                                            <td class="col-md-1 ">@item.Valor</td>
                                            <td>
                                                <a class="btn btn-danger btn-sm btnDelete" onclick="remove(this);">
                                                    <span class="glyphicon glyphicon-trash"></span>
                                                </a>
                                            </td>
                                        </tr>
                                    }
                                }
                            <tbody id="Items"></tbody>
                            

                        </table>
                    </div>
                </div>

                <div class="row" style="margin-top: 10px;">
                    <div class=" row" style="margin-right: 0px">
                        <div class="col-md-7"></div>
                        <div class="col-md-5">
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <div class="col-md-7">
                                        <h4>Gastos Reportados:</h4>
                                        <h4>Anticipo Entregado:</h4>
                                    </div>
                                    <div class="col-md-5">
                                        <h4 class="text-right" id="txMontoT"> 0</h4>
                                        <h4 class="text-right" id="txSaldo"> @Model.Monto</h4>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6"></div>
                    <div class="col-md-2">
                        <button type="button" class="btn btn-default" style="float:right;"><span class="glyphicon glyphicon-plus"></span>Cancelar</button>
                    </div>
                    <div class="col-md-2">
                        <button type="submit" class="btn btn-primary" onclick="return cargarGastos();">
                            <span class="glyphicon glyphicon-plus"></span> Guardar Reporte
                        </button>

                    </div>
                    <div class="col-md-2">
                        <button type="button" class="btn btn-primary" onclick="EnviarReporte(this);" style="float:right;"><span class="glyphicon glyphicon-plus"></span>Enviar Reporte</button>
                    </div>
                </div>

            </div>
        </div>

    </section>
</form>

@*Aqui viene el formulario modal del listado de gastos*@
<!--modal form-->
<div class="row">
    <div class="col-md-9">
        <div class="modal fade bs-example-modal-sm" id="gastosModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="class=" row margin-bottom"">
                        <p>
                            <br />
                        </p>
                    </div>
                    <div class="box-header with-border">
                        <h3 class="box-title"><i class="fa fa-list-alt"></i>Seleccionar Gasto</h3>
                        <div class="box-tools pull-right">
                            <a id="btnAddNuevoGasto" data-dismiss="modal" class="btn btn-primary" onclick="AddNuevoGasto();">
                                <span class="glyphicon glyphicon-plus"></span> Añadir Nuevo Gasto
                            </a>

                        </div>
                    </div>



                    <div class="class=" row margin-bottom"">
                        <p>
                            <br />
                        </p>
                    </div>

                    <div class="modal-body">
                        <div class="form-horizontal">
                            @if (Model.SolicitudGastos != null)
                            {
                                if (Model.SolicitudGastos.Count > 0)
                                {
                                    <div class="row ListadoGastos">
                                        <div class="col-md-12">
                                            <table class="col-md-12">
                                                <thead>
                                                    <tr>
                                                        <td>ID</td>
                                                        <td>Fecha</td>
                                                        <td>Pais</td>
                                                        <td>servicio</td>
                                                        <td>Ciudad</td>
                                                        <td>Origen</td>
                                                        <td>Destino</td>
                                                        <td>Valor</td>
                                                        <td>Acciones</td>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model.SolicitudGastos)
                                                    {

                                                        <tr>
                                                            <td class="col-md-1 ">@item.Id</td>
                                                            @*<td class="col-md-2 ">@String.Format("{0:dd-MM-yyy}", item.FechaGasto)</td>*@
                                                            <td class="col-md-2 ">@item.FechaGasto.Substring(0, 10)</td>
                                                            <td class="col-md-1 ">@item.Pais</td>
                                                            <td class="col-md-1 ">@item.Servicio</td>
                                                            <td class="col-md-1 ">@item.Ciudad</td>

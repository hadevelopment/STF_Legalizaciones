@model IEnumerable<Legalizaciones.Model.SolicitudGastos>

@{
    ViewData["Title"] = "SolicitudItem";
}

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FechaGasto.ToShortDateString()
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pais)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Servicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ciudad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Origen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Destino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Monto) 
            </td>
        </tr>
    }
